<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">
    <description>
    	Configure an extra TicketRegistry for the
    	casShibCentralAuthenticationService.  This is to keep CASShib
    	tickets separate from "regular CAS" tickets.
    </description>
       
    <!-- Ticket Registry -->
	<bean id="casShibTicketRegistry" class="org.jasig.cas.ticket.registry.DefaultTicketRegistry" />
	
</beans>